package Ch04;

public class Book {

	String title;
	String author;
	
	public Book (String t) {
		title = t;
		author = "작자미상";
	}
	
	public Book (String t, String a) {
		title = t;
		author = a;
	}
	
	public static void main(String[] args) {
		Book b1 = new Book("book1");
		Book b2 = new Book("book2", "author");
		
		System.out.println("책1: " + b1);
	}
}
